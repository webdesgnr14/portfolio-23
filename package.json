  "license": "MIT",
  "scripts": {
    "build:js": "webpack --env production --mode production",
    "build:js-dev": "webpack --env development --mode development",
    "build:css": "node-sass src/scss/app.scss --output-style compressed --importer node_modules/node-sass-package-importer/dist/cli.js -o build && postcss build/app.css --replace",
    "build": "yarn build:js && yarn build:css",
    "watch:js": "watch 'yarn build:js-dev' src/",
    "watch:css": "nodemon -e scss -x \" yarn build:css\"",
    "dev": "yarn watch:css & yarn watch:js",
    "sync": "browser-sync start -p 'localhost' --files './build/*.js' './build/*.css' '**/*.php'"
  },
  "dependencies": {
